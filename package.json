{
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/plugin-transform-modules-commonjs": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "babel-jest": "^27.3.1",
        "cypress": "^9.0.0",
        "eslint": "^8.3.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-jest": "^25.2.4",
        "eslint-plugin-json": "^3.1.0",
        "htmlhint": "^0.16.1",
        "jest": "^27.3.1",
        "jest-puppeteer": "^6.0.0",
        "markdownlint-cli": "^0.30.0",
        "nodemon": "^2.0.15",
        "prettier": "2.4.1",
        "puppeteer": "^11.0.0",
        "start-server-and-test": "^1.14.0",
        "stylelint": "^14.1.0",
        "stylelint-config-standard": "^24.0.0"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js & start-server-and-test start http://localhost:3030 cy:run",
        "cy:run": "cypress run",
        "start": "node --experimental-modules source/service/server/server.mjs",
        "dev": "nodemon source/service/server/server.js",
        "eslint": "npx eslint '**/*.js' '**/*.json' '**/*.mjs'",
        "stylelint": "npx stylelint '**/*.css'",
        "htmlhint": "npx htmlhint '**/*.html'",
        "prettier": "npx prettier --write '**/*.js'",
        "lint": "npm run eslint && npm run stylelint && npm run htmlhint"
    },
    "jest": {
        "preset": "jest-puppeteer",
        "verbose": true,
        "testRegex": "__tests__/.*\\.test\\.js$",
        "transform": {}
    },
    "dependencies": {
        "fastify": "^3.24.0",
        "fastify-cors": "^6.0.2",
        "fastify-static": "^4.5.0",
        "fs": "^0.0.1-security",
        "ignore-errors": "^2.0.0",
        "import-http": "^0.3.1",
        "nedb": "^1.8.0",
        "path": "^0.12.7"
    },
    "type": "module"
}
